@model Presentation.Web.Models.HomeModel
@{
    ViewData["Title"] = "Home Page";
}

<div class="leaderboard">
    <h1>
        <svg class="ico-cup">
            <use xlink:href="#cup"></use>
        </svg>
        @Model.Club.Name
    </h1>
    <ol>
        @foreach (var member in Model.Club.Members.Take(5))
        {
        <li>
            <img src="@member.AvatarUrl" alt="..." width="50px" height="50px">
            <mark>@member.Name</mark>
            <small>@member.Trophies</small>
        </li>
        }
    </ol>
</div>
